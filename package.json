{
  "scripts": {
    "mochawesome": "./node_modules/.bin/mocha --reporter mochawesome",
    "dev": "node index.js",
    "start:dev": "node  -r dotenv/config index.js dotenv_config_path=.env.dev",
    "start:test": "node -r dotenv/config index.js dotenv_config_path=.env.test",
    "farm:prod": "node -r dotenv/config farm dotenv_config_path=.env.prod",
    "build:one": "node --inspect-brk -r dotenv/config scripts/build.js dotenv_config_path=.env.one",
    "tests": "node scripts/test.js",
    "test": "export NODE_ENV=test PORT=3300 && mocha --timeout 100000 'test/**/*.test.js' --exit",
    "t": "export NODE_ENV=test PORT=3300 && mocha --timeout 100000 -g 'get_eth_pairs_info'  --exit",
    "t1": "export NODE_ENV=test PORT=3300 && mocha --timeout 100000 -g 'get_pairs_info'  --exit",
    "t2": "export NODE_ENV=test PORT=3300 && mocha --timeout 100000 -g 'get_swp_info'  --exit",
    "t3": "export NODE_ENV=test PORT=3300 && mocha --timeout 100000 -g 'get_swp_balance'  --exit",
    "t4": "export NODE_ENV=test PORT=3300 && mocha --timeout 1000000 -g 'get_pairtokens_info'  --exit",
    "cov": "export NODE_ENV=test PORT=3300 && nyc mocha -f 'option'  --exit",
    "h": "mocha --help --exit"
  },
  "dependencies": {
    "co": "^4.6.0",
    "co-mysql": "^1.0.0",
    "connect-timeout": "^1.9.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mocha": "^8.1.3",
    "mochawesome": "^6.1.1",
    "mysql": "^2.10.1",
    "node-cron": "^2.0.3",
    "power-assert": "^1.6.1",
    "should": "^13.2.3",
    "sleep": "^6.3.0",
    "supertest": "^4.0.2",
    "web3": "^1.2.11"
  }
}
